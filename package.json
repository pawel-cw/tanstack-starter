{
  "name": "tanstack-starter",
  "version": "1.0.0",
  "description": "A modern, production-ready boilerplate for building full-stack applications with TanStack Start",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite dev --port 3000",
    "build": "vite build",
    "serve": "vite preview",
    "test": "vitest run",
    "lint": "eslint",
    "format": "prettier",
    "check": "prettier --write . && eslint --fix",
    "typecheck": "tsc --noEmit",
    "trans:extract": "lingui extract",
    "trans:compile": "lingui compile --typescript",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:push": "drizzle-kit push",
    "db:pull": "drizzle-kit pull",
    "db:studio": "drizzle-kit studio"
  },
  "dependencies": {
    "@lingui/core": "^5.5.2",
    "@lingui/react": "^5.5.2",
    "@neondatabase/serverless": "^1.0.2",
    "@tailwindcss/vite": "^4.1.16",
    "@tanstack/react-devtools": "^0.8.0",
    "@tanstack/react-query": "^5.90.7",
    "@tanstack/react-query-devtools": "^5.90.2",
    "@tanstack/react-router": "^1.133.36",
    "@tanstack/react-router-devtools": "^1.134.12",
    "@tanstack/react-router-ssr-query": "^1.134.9",
    "@tanstack/react-start": "^1.134.7",
    "@tanstack/router-plugin": "^1.135.0",
    "better-auth": "^1.3.34",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "drizzle-kit": "^0.31.6",
    "drizzle-orm": "^0.44.7",
    "lucide-react": "^0.552.0",
    "radix-ui": "^1.4.3",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "tailwind-merge": "^3.3.1",
    "tailwindcss": "^4.1.16",
    "tw-animate-css": "^1.4.0",
    "vite-tsconfig-paths": "^5.1.4",
    "ws": "^8.18.3",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@lingui/babel-plugin-lingui-macro": "^5.5.2",
    "@lingui/cli": "^5.5.2",
    "@lingui/vite-plugin": "^5.5.2",
    "@tanstack/eslint-config": "^0.3.2",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/react": "^16.3.0",
    "@types/node": "^24.10.0",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "@types/ws": "^8.18.1",
    "@vitejs/plugin-react": "^5.1.0",
    "dotenv": "^17.2.3",
    "jsdom": "^27.1.0",
    "prettier": "^3.6.2",
    "typescript": "^5.9.3",
    "vite": "^7.2.2",
    "vitest": "^4.0.6",
    "web-vitals": "^5.1.0"
  }
}
